{
  "name": "ddntj",
  "version": "1.0.0",
  "description": "Playing online TRPG with HTML5",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "forever start -w app.js && npm run watch",
    "stop": "forever stop app.js",
    "watch": "NODE_ENV=dev webpack --watch --config webpack/webpack.base.conf.js",
    "build": "NODE_ENV=prod webpack --config webpack/webpack.base.conf.js",
    "build:dev": "NODE_ENV=dev webpack --config webpack/webpack.base.conf.js",
    "heroku-postbuild": "npm run build",
    "heroku-prebuild": "npm install --only=dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Minori-Akizuki/ddnty.git"
  },
  "author": "Minori Akizuki",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Minori-Akizuki/ddnty/issues"
  },
  "homepage": "https://github.com/Minori-Akizuki/ddnty#readme",
  "dependencies": {
    "forever": "^0.15.3",
    "jquery": "^3.2.1",
    "log4js": "^2.3.3",
    "socket.io": "^2.0.3",
    "vue": "^2.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.5",
    "html-webpack-plugin": "^2.30.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.4",
    "vue-router": "^2.7.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.5"
  }
}
